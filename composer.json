{
    "name": "erth0/php-google-static-map",
    "description": "A URL builder for Google Maps Static.",
    "keywords": [
        "Erth0",
        "php-google-static-map"
    ],
    "homepage": "https://github.com/erth0/php-google-static-map",
    "license": "MIT",
    "authors": [
        {
            "name": "Eluert Mukja",
            "email": "hey@mukja.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Erth0\\PhpGoogleStaticMap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Erth0\\PhpGoogleStaticMap\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
